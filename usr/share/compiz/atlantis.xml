<?xml version="1.0" encoding="UTF-8"?>
<compiz>
	<plugin useBcop="true" name="atlantis">
		<short>Cube Atlantis</short>
		<short xml:lang="el">Κύβος Ατλαντίς</short>
		<short xml:lang="en_GB">Cube Atlantis</short>
		<short xml:lang="eu">Atlantis kuboa</short>
		<short xml:lang="fr">Cube Atlantis</short>
		<short xml:lang="gl">Atlántida no Cubo</short>
		<short xml:lang="it">Cubo Atlantide</short>
		<short xml:lang="ja">キューブ・アトランティス</short>
		<short xml:lang="nb">Atlantiskube</short>
		<short xml:lang="nl">Kubus Atlantis</short>
		<short xml:lang="pt">Cubo Atlantis</short>
		<short xml:lang="sv">Kub Atlantis</short>
		<long>Render some fish inside of the transparent cube</long>
		<long xml:lang="el">Απεικόνιση μερικών ψαριών μέσα στον διάφανο κύβο</long>
		<long xml:lang="en_GB">Render some fish inside of the transparent cube</long>
		<long xml:lang="eu">Errendatu arrain batzuk kubo garden barnean.</long>
		<long xml:lang="fr">Afficher des poissons à l'intérieur du cube transparent</long>
		<long xml:lang="gl">Mostrar algúns peixes dentro do cubo transparente</long>
		<long xml:lang="it">Rappresenta dei pesci all'interno del cubo trasparente</long>
		<long xml:lang="ja">透明なキューブ内に魚を描画</long>
		<long xml:lang="nb">Gjengir noen fisk på innsiden av den gjennomsiktige kuben</long>
		<long xml:lang="nl">Teken een paar vissen binnenin de transparante kubus</long>
		<long xml:lang="pl">Wyświetla niektóre ryby wewnątrz przezroczystej kostki.</long>
		<long xml:lang="pt">Mostrar peixes dentro do cubo transparente</long>
		<long xml:lang="sv">Renderera fiskar inuti den genomskinliga kuben</long>
		<long xml:lang="zh_CN">在透明立方体中渲染一些鱼</long>
		<category>Effects</category>
		<deps>
		<relation type="after">
			<plugin>cube</plugin>
		</relation>
		<requirement>
			<plugin>cube</plugin>
		</requirement>
		</deps>
		<screen>
			<group>
				<short>Water / Ground</short>
				<option type="bool" name="show_water">
					<short>Render water</short>
					<long>Render water inside the cube.</long>
					<default>true</default>
				</option>
				<option type="bool" name="show_water_wire">
					<short>Render water wireframe</short>
					<long>Render water wireframe inside the cube.</long>
					<default>false</default>
				</option>
				<option type="bool" name="show_ground">
					<short>Render ground</short>
					<long>Render ground inside the cube.</long>
					<default>true</default>
				</option>
				<option type="float" name="water_height">
					<short>Water surface height</short>
					<long>Height of water surface in the cube.</long>
					<default>0.85</default>
					<min>0.2</min>
					<max>1</max>
					<precision>0.01</precision>
				</option>
				<option type="color" name="water_color">
					<short>Water opacity</short>
					<long>Opacity of water in the cube.</long>
					<default>
						<red>0x0800</red>
						<green>0x0800</green>
						<blue>0x3000</blue>
						<alpha>0xf00</alpha>
					</default>
				</option>
				<option type="int" name="grid_quality">
					<short>Water/Ground grid smoothnes</short>
					<long>Number of subdivisions that create a finer grid.</long>
					<default>3</default>
					<min>1</min>
					<max>10</max>
				</option>

				<subgroup>
					<short>Waves</short>
					<option type="bool" name="render_waves">
						<short>Render waves</short>
						<long>Render water waves inside the cube.</long>
						<default>true</default>
					</option>
					<option type="float" name="wave_amplitude">
						<short>Wave amplitude</short>
						<long>Amplitude of water waves.</long>
						<default>0.05</default>
						<min>0</min>
						<max>0.5</max>
						<precision>0.01</precision>
					</option>
					<option type="float" name="wave_frequency">
						<short>Wave frequency</short>
						<long>Frequency of water waves.</long>
						<default>20</default>
						<min>0</min>
						<max>100</max>
						<precision>0.1</precision>
					</option>
					<option type="float" name="wave_speed">
						<short>Wave speed</short>
						<long>Speed of water waves.</long>
						<default>1</default>
						<min>0</min>
						<max>10</max>
						<precision>0.01</precision>
					</option>
					<option type="float" name="small_wave_amplitude">
						<short>Small wave amplitude</short>
						<long>Amplitude of small water waves on the bigger ones.</long>
						<default>0.02</default>
						<min>0</min>
						<max>0.5</max>
						<precision>0.01</precision>
					</option>
					<option type="float" name="small_wave_frequency">
						<short>Small wave frequency</short>
						<long>Frequency of small water waves on the bigger ones.</long>
						<default>40</default>
						<min>0</min>
						<max>100</max>
						<precision>0.1</precision>
					</option>
					<option type="float" name="small_wave_speed">
						<short>Wave Speed</short>
						<long>Speed of small water waves.</long>
						<default>1</default>
						<min>0</min>
						<max>10</max>
						<precision>0.01</precision>
					</option>
				</subgroup>
				
			</group>
