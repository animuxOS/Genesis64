# xfce-setting-show provided by xfce4-mcs-manager wich we depend on.
xfce4-mcs-plugins: desktop-command-not-in-package /usr/share/applications/xfce-display-settings.desktop xfce-setting-show
xfce4-mcs-plugins: desktop-command-not-in-package /usr/share/applications/xfce-keyboard-settings.desktop xfce-setting-show
xfce4-mcs-plugins: desktop-command-not-in-package /usr/share/applications/xfce-mouse-settings.desktop xfce-setting-show
xfce4-mcs-plugins: desktop-command-not-in-package /usr/share/applications/xfce-ui-settings.desktop xfce-setting-show

