<?xml version="1.0" encoding="UTF-8"?>
<section>
	<sec_name>Trash</sec_name>
	<sec_name xml:lang="bg">Кошче</sec_name>
	<sec_name xml:lang="de">Mülleimer</sec_name>
	<sec_name xml:lang="es">Papelera</sec_name>
	<sec_name xml:lang="fr">Corbeille</sec_name>
	<sec_name xml:lang="it">Cestino</sec_name>
	<sec_name xml:lang="ja">ゴミ箱</sec_name>
	<sec_name xml:lang="pt">Lixeira</sec_name>
	<sec_desc>Trash Icon for Kiba.</sec_desc>
	<sec_desc xml:lang="bg">Икона на кошчето за Киба-док</sec_desc>
	<sec_desc xml:lang="de">Mülleimer Symbol für Kiba.</sec_desc>
	<sec_desc xml:lang="es">Icono de la papelera para Kiba.</sec_desc>
	<sec_desc xml:lang="fr">Icône de la corbeille Kiba</sec_desc>
	<sec_desc xml:lang="it">Icona del Cestino per Kiba.</sec_desc>
	<sec_desc xml:lang="ja">Kiba-Dock のゴミ箱アイコン</sec_desc>
	<sec_desc xml:lang="pt">Ícone da lixeira para o Kiba.</sec_desc>
	<sec_icon>[gnome-fs-trash-empty]</sec_icon>
	<subsection>
		<sub_sec_name>General</sub_sec_name>
		<sub_sec_name xml:lang="bg">Общи</sub_sec_name>
		<sub_sec_name xml:lang="de">Generell</sub_sec_name>
		<sub_sec_name xml:lang="es">General</sub_sec_name>
		<sub_sec_name xml:lang="fr">Général</sub_sec_name>
		<sub_sec_name xml:lang="it">Generale</sub_sec_name>
		<sub_sec_name xml:lang="ja">全般</sub_sec_name>
		<sub_sec_name xml:lang="pt">Geral</sub_sec_name>
		<option>
			<name>trash_enable</name>
			<title>Enable</title>
			<title xml:lang="bg">Пускане</title>
			<title xml:lang="de">Aktivieren</title>
			<title xml:lang="es">Activar</title>
			<title xml:lang="fr">Activer</title>
			<title xml:lang="it">Abilita</title>
			<title xml:lang="ja">有効</title>
			<title xml:lang="pt">Habilitar</title>
			<type>bool</type>
			<range/>
			<default>TRUE</default>
			<description>Enables the Trash plugin</description>
			<description xml:lang="bg">Пуска плъгина "Кошче"</description>
			<description xml:lang="de">Aktiviere das Mülleimer Plugin</description>
			<description xml:lang="ja">ゴミ箱プラグインの有効化/無効化</description>
		</option>
		<option>
			<name>trash_size_factor</name>
			<title>Size factor</title>
			<title xml:lang="bg">Степен на оразмеряване</title>
			<title xml:lang="de">Größen Faktor</title>
			<title xml:lang="ja">アイコンの大きさ</title>
			<type>double</type>
			<range>0.1-2.0</range>
			<default>1.0</default>
			<description>Size factor</description>
			<description xml:lang="bg">Степен на оразмеряване</description>
			<description xml:lang="de">Größen Faktor</description>
			<description xml:lang="ja">アイコンの大きさ</description>
		</option>
		<option>
			<name>trash_alpha</name>
			<title>Alpha</title>
			<title xml:lang="bg">Прозрачност</title>
			<title xml:lang="de">Transparenz</title>
			<title xml:lang="es">Transparencia</title>
			<title xml:lang="it">Alpha</title>
			<title xml:lang="ja">透明度</title>
			<title xml:lang="pt">Transparência</title>
			<type>double</type>
			<range>0.0-1.0</range>
			<default>1.0</default>
			<description>Amount of Icon alpha</description>
			<description xml:lang="bg">Прозрачност на иконата на</description>
			<description xml:lang="de">Wert der Symbol Transparenz</description>
			<description xml:lang="ja">ゴミ箱アイコンの透明度</description>
		</option>
	</subsection>
