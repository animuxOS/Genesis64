                                        </local_schema>
                                </entry>
                                <entry name="host">
                                        <local_schema short_desc="HTTP პროქსის ჰოსტის სახელი">
                                                <longdesc>პროქსი სერვერის მანქანური სახელი HTTP-სთვის.</longdesc>
                                        </local_schema>
